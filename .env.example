# OpenAI/Anthropic (for advanced analysis)
OPENAI_API_KEY=your_openai_key
ANTHROPIC_API_KEY=your_anthropic_key

# Output Configuration
OUTPUT_DIRECTORY=./reports
LOG_LEVEL=INFO
