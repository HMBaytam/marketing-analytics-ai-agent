# Google Ads API Configuration
# Get these from Google Ads API Center: https://developers.google.com/google-ads/api/docs/first-call/overview
GOOGLE_ADS_DEVELOPER_TOKEN=your_developer_token_here
GOOGLE_ADS_CLIENT_ID=your_client_id_here.apps.googleusercontent.com
GOOGLE_ADS_CLIENT_SECRET=your_client_secret_here
GOOGLE_ADS_REFRESH_TOKEN=your_refresh_token_here
GOOGLE_ADS_CUSTOMER_ID=1234567890
GOOGLE_ADS_LOGIN_CUSTOMER_ID=9876543210
GOOGLE_ADS_USE_PROTO_PLUS=true

# Google Analytics 4 Configuration
# Get property ID from GA4 admin panel
GOOGLE_ANALYTICS_PROPERTY_ID=123456789
GOOGLE_ANALYTICS_CLIENT_ID=your_client_id_here.apps.googleusercontent.com
GOOGLE_ANALYTICS_CLIENT_SECRET=your_client_secret_here
GOOGLE_ANALYTICS_REFRESH_TOKEN=your_refresh_token_here
# Alternative: use service account credentials
GOOGLE_ANALYTICS_CREDENTIALS_PATH=config/secrets/ga4-service-account.json

# Anthropic API Configuration
# Get API key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=sk-ant-api03-your_api_key_here
ANTHROPIC_MODEL=claude-3-sonnet-20240229
ANTHROPIC_MAX_TOKENS=8000
ANTHROPIC_TEMPERATURE=0.3

# Application Configuration
LOG_LEVEL=INFO
OUTPUT_DIR=./reports
CACHE_DIR=./.cache
RATE_LIMIT_REQUESTS_PER_MINUTE=60